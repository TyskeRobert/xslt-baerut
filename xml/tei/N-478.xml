                            <me:norm>konungr</me:norm> <!-- OEH 2021-03-25: Trur eg ville ha valt den tradisjonelle forma “konungr” -->
                        </choice>
                    </w>
                    <pc type="runic">
                        <choice>
                            <me:facs>&twodotPM;</me:facs>
                        </choice>
                    </pc>
                    <w lemma="hjálpa" me:msa="xVB fF tPS mSU p3 nP vA iST">
                        <choice>
                            <me:facs>&hYGL;&iGE;&aYG;&lGE;&pMD;&eMD;</me:facs>
                            <me:dipl>hialpe</me:dipl>
                            <me:norm>hjalpi</me:norm>
                        </choice>
                    </w>
                    <pc type="runic">
