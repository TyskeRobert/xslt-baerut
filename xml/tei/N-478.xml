                    <w lemma="þessi" me:msa="xDD gF nP cA">
                        <choice>
                            <me:facs>&thGE;&eMD;<unclear reason="damage">&sYGSdot;</unclear>&aYG;&rGE;</me:facs>
                            <me:dipl>þe<supplied type="restoration" resp="MagnusOlsen">s</supplied>ar</me:dipl>
                            <me:norm>þe<supplied type="restoration" resp="MagnusOlsen">ss</supplied>ar</me:norm>
                        </choice>
                    </w>
                    <pc type="runic">
                        <choice>
                            <me:facs>&twodotPM;</me:facs>
                        </choice>
                    </pc>
                    <w lemma="rún" me:msa="xNC gF nP cA sI">
                        <choice>
                            <me:facs>&rGE;&uGE;&nYG;&aYG;&rGE;</me:facs>
                            <me:dipl>runar</me:dipl>
                            <me:norm>rúnar</me:norm>
                        </choice>
                    </w>
                    <pc type="runic">
                        <choice>
                            <me:facs>&twodotPM;</me:facs>
                        </choice>
                    </pc>
                    <w lemma="rísta" me:msa="xVB fF tPT mIN p3 nS vA iST">
                        <choice>
                            <me:facs>&rGE;&aeMD;&iGE;&sYGSdot;&tYG;</me:facs>
                            <me:dipl>ræist</me:dipl>
                            <me:norm>reist</me:norm>
                        </choice>
                    </w>
                    <pc type="runic">
                        <choice>
                            <me:facs>&twodotPM;</me:facs>
                            <me:norm>,</me:norm>
                        </choice>
                    </pc>
                    <w lemma="með" me:msa="xAP yD">
                        <choice>
                            <me:facs>&mYGLst;&eMD;&thGE;&rGE;</me:facs>
                            <me:dipl>meþr</me:dipl>
                            <me:norm>með</me:norm>
