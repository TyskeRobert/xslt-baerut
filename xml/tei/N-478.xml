                        </choice>
                    </w>
                    <pc type="runic">
                        <choice>
                            <me:facs>&twodotPM;</me:facs>
                        </choice>
                    </pc>
                    <w lemma="Ólafr" me:msa="xNP gM nS cN sI">
                        <choice>
                            <me:facs>&oYG;&lGE;&aYG;&fGEst;&rGE;</me:facs>
