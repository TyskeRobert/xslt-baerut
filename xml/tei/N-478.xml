                    </w>
                    <pc type="runic">
                        <choice>
                            <me:facs>&twodotPM;</me:facs>
                        </choice>
                    </pc>
                    <w lemma="konungr" me:msa="xNC gM nS cN sI">
                        <choice>
                            <me:facs>&kYGst;&oYG;&nYG;&gMDst;&rGE;</me:facs>
