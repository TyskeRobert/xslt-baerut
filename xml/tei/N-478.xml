                            <me:facs>&middotPM;</me:facs>
                        </choice>
                    </pc>
                    <w lemma="ok" me:msa="xCC">
                        <choice>
                            <me:facs>&oYG;&kORyshp;</me:facs>
                            <me:dipl>ok</me:dipl>
                            <me:norm>ok</me:norm>
                        </choice>
                    </w>
                     <pc type="runic">
                        <choice>
                            <me:facs>&twodotPM;</me:facs>
                        </choice>
                    </pc>
                    <w lemma="hinn" me:msa="xDD gM nS cN">
                        <choice>
                            <me:facs>&hYGL;&iGE;&nYG;</me:facs>
                            <me:dipl>hin</me:dipl>
                            <me:norm>hinn</me:norm>
                        </choice>
                    </w>
                    <pc type="runic">
                        <choice>
                            <me:facs>&twodotPM;</me:facs>
                        </choice>
                    </pc>
                    <w lemma="helgr" me:msa="xAJ rP gM nS cN sD">
                        <choice>
                            <me:facs>&hYGL;&iGE;&aeMD;&lGE;&gMDst;&iGE;</me:facs>
                            <me:dipl>hiælgi</me:dipl>
