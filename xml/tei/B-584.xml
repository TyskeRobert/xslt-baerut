	<text xml:lang="nor">
		<front>
			<div type="abstracts">
				<head><hi rend="bold">Signum</hi>: B 584 || <hi rend="bold">Sted</hi>: Dreggen (Bryggen), Bergen, Hordaland || <hi rend="bold">Datering</hi>: ca. 1250–1300 || <hi rend="bold">Kart</hi>: På vei<lb/>
					<hi rend="bold">Materiale</hi>: Tre || <hi rend="bold">Status</hi>: Bevart, nå i Universitetsmuseet i Bergen || <hi rend="bold">Utgave</hi>: Ikke i NIyR || <hi rend="bold">SamNord</hi>: <ref target="http://kulturarvsdata.se/uu/srdb/a6c8f49f-e3ad-40e8-b0ad-857f9ef594d1" type="external">N B584</ref>
				</head>
				</div>
			<div type="presentation">
				<head>Omtale</head>
				<p>Denne innskriften er av tydelig humoristisk karakter.</p>
				<note type="explanation"><ref target="pdf/Runekatalog-presentasjon.pdf">Forklaring på oppsettet av runeinnskriftene</ref></note>
			</div>
		</front>
		<body>
			<div type="runic-inscription">
				<head>Transkripsjon, translitterering og normalisering av innskriften</head>
				<p>
					<pb ed="ms" n="1r"/>
					<lb ed="ms" n="1"/>
					<w lemma="setja" me:msa="xVB fF tPS mIP p2 nS vR iWK">
						<!--Her er jeg usikker på om msa er korrekt. Anna -->
						<choice>
							<me:facs><unclear reason="weathered">&sYGS;&aeMD;&sYGS;&tOR;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">sæst</supplied></me:dipl>
							<me:norm><supplied reason="restoration" resp="unknown" source="SamNord">Sezt</supplied></me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs><unclear reason="weathered">&threedotPM;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">&threedotPM;</supplied></me:dipl>
						</choice>
					</pc>
					<w lemma="niðr" me:msa="xAV rP">
						<choice>
							<me:facs><unclear reason="weathered">&nYG;</unclear>&iGE;&thGE;&nOR;<unclear reason="weathered">&rGE;</unclear></me:facs>
							<!-- SP 2022-02-27: the æ rune is clearly mirrored, thus nOR. -->
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">n</supplied>iþæ<supplied reason="restoration" resp="unknown" source="SamNord">r</supplied></me:dipl>
							<me:norm><supplied reason="restoration" resp="unknown" source="SamNord">n</supplied>ið<supplied reason="restoration" resp="unknown" source="SamNord">r</supplied></me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs><unclear reason="weathered">&threedotPM;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">&threedotPM;</supplied></me:dipl>
						</choice>
					</pc>
					<w lemma="ok" me:msa="xCC">
						<choice>
							<me:facs><unclear reason="weathered"><seg type="lig">&oYG;&#xF538;&kYG;</seg></unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord"><seg type="lig">o&#xF538;k</seg></supplied></me:dipl>
							<me:norm><supplied reason="restoration" resp="unknown" source="SamNord">ok</supplied></me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs><unclear reason="weathered">&middotPM;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">&middotPM;</supplied></me:dipl>
						</choice>
					</pc>
					<w lemma="ráða" me:msa="xVB fF tPS mIP p2 nS vA iST">
						<choice>
							<me:facs>&rGE;<unclear reason="weathered">&aYG;</unclear>&tOR;</me:facs>
							<me:dipl>r<supplied reason="restoration" resp="unknown" source="SamNord">a</supplied>t</me:dipl>
							<me:norm>r<supplied reason="restoration" resp="unknown" source="SamNord">á</supplied>ð</me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs><unclear reason="weathered">&threedotPM;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">&threedotPM;</supplied></me:dipl>
						</choice>
					</pc>
					<w lemma="rún" me:msa="xNC gF nP cA sI">
						<choice>
							<me:facs>&rGE;&uGE;&nYG;<seg type="lig">&aYG;&#xF538;&rGE;</seg></me:facs>
							<!-- SP 2022-02-27: Fixed lig.-->
							<me:dipl>run<seg type="lig">a&#xF538;r</seg></me:dipl>
							<me:norm>rúnar</me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs><unclear reason="weathered">&threedotPM;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">&threedotPM;</supplied></me:dipl>
							<me:norm>,</me:norm>
						</choice>
					</pc>
					<w lemma="rísa" me:msa="xVB fF tPS mIP p2 nS vA iST">
						<choice>
							<me:facs>&rGE;&iGE;&sYGS;</me:facs>
							<me:dipl>ris</me:dipl>
							<me:norm>rís</me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs><unclear reason="weathered">&threedotPM;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">&threedotPM;</supplied></me:dipl>
						</choice>
					</pc>
					<w lemma="upp" me:msa="xAV rP">
						<choice>
							<me:facs>&uGE;&pMDopen;</me:facs>
							<me:dipl>up</me:dipl>
							<me:norm>upp</me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs>&middotPM;</me:facs>
							<me:dipl>&middotPM;</me:dipl>
						</choice>
					</pc>
					<w lemma="ok" me:msa="xCC">
						<choice>
							<me:facs><seg type="lig">&oYG;&#xF538;&kYG;</seg></me:facs>
							<me:dipl><seg type="lig">o&#xF538;k</seg></me:dipl>
							<me:norm>ok</me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs>&middotPM;</me:facs>
							<me:dipl>&middotPM;</me:dipl>
						</choice>
					</pc>
					<w lemma="físa" me:msa="xVB fF tPS mIP p2 nS vA iST">
						<choice>
							<me:facs>&fGE;&iGE;&sYGSdot;</me:facs>
							<me:dipl>fis</me:dipl>
							<me:norm>fís</me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs><unclear reason="weathered">&twodotPM;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">&twodotPM;</supplied></me:dipl>
						</choice>
					</pc>
					<w lemma="við" me:msa="xAV">
						<choice>
							<me:facs>&uGE;&iGE;&tOR;</me:facs>
							<me:dipl>uit</me:dipl>
							<me:norm>við</me:norm>
						</choice>
					</w>
					<pc type="runic">
						<choice>
							<me:facs><unclear reason="weathered">&threedotPM;</unclear></me:facs>
							<me:dipl><supplied reason="restoration" resp="unknown" source="SamNord">&threedotPM;</supplied></me:dipl>
							<me:norm>!</me:norm>
						</choice>
					</pc>
				</p>
			</div>
		</body>
		<back>
			<div type="translation" xml:lang="nor">
				<head>Oversettelse til norsk</head>
				<p>Sett deg ned og tolk runene, reis deg opp og fis!</p>
			</div>
			<div type="translation" xml:lang="eng">
				<head>Oversettelse til engelsk (etter innførselen i SamNord)</head>
				<p>Sit down and interpret the runes, stand up and fart!</p>
			</div>
			<div type="comment" xml:lang="nor">
				<head>Kommentar</head>
				<p>Rune &aeMD; <hi rend="bold">æ</hi> i ordet &nYG;&iGE;&thGE;&aeMD;&rGE; <hi rend="bold">niþær</hi> er en venderune.</p>
			</div>
