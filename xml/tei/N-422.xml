						<choice>
							<me:facs>&gMD;&uGE;&lGE;&lGE;&iGE;</me:facs>
							<me:dipl>gulli</me:dipl>
							<me:norm>gulli</me:norm>
						</choice>
					</w>
					
					<lb ed="ms" n="C"/>
					
					<w lemma="ok" me:msa="xCC">
						<choice>
							<me:facs><seg type="lig">&oYG;&#xF538;&kYG;</seg></me:facs>
							<me:dipl><seg type="lig">o&#xF538;k</seg></me:dipl>
							<me:norm>ok</me:norm>
						</choice>
					</w>

					<w lemma="sá" me:msa="xDD gN nS cN">
						<choice>
							<me:facs>&thGE;&eMD;&tYG;</me:facs>
							<me:dipl>þet</me:dipl>
							<me:norm>þat</me:norm>
						</choice>
					</w>

					<w lemma="vera" me:msa="xVB fF tPS mIN p3 nS vA iST">
						<choice>
							<me:facs>&eMD;&rGE;</me:facs>
							<me:dipl>er</me:dipl>
							<me:norm>er</me:norm>
						</choice>
					</w>

					<w lemma="í" me:msa="xAP yD">
						<choice>
							<me:facs>&iGE;</me:facs>
							<me:dipl>i</me:dipl>
							<me:norm>í</me:norm>
						</choice>
					</w>

					<w lemma="þessi" me:msa="xDD gM nS cD">
						<choice>
							<me:facs>&thGE;&eMD;&sYGSdot;&uGE;&mYGL;</me:facs>
