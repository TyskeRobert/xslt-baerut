                                <me:dipl>f<seg type="lig">o&#xF538;r</seg></me:dipl>
                                <me:norm>fór</me:norm>
                            </choice>
                        </w>
                        <w lemma="hann" me:msa="xPE p3 gM nS cN">
                            <choice>
                                <me:facs><add place="supralinear">&hYGL;</add>&aYG;&nYG;</me:facs>
